{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-1project"
		},
		"AzureDataLakeStorage1_GitHub_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1_GitHub'"
		},
		"AzureSynapseAnalytics1_stackoverflow_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSynapseAnalytics1_stackoverflow'"
		},
		"AzureDataLakeStorage1_GitHub_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://decoursesacc.dfs.core.windows.net"
		},
		"RestService1_Company_detail_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://de-course-ingest-api.azurewebsites.net/api/"
		},
		"local_ADLS_project1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storage1project.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-1project_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_GitHub_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_GitHub_accountKey')]"
					},
					"AzureSynapseAnalytics1_stackoverflow_connectionString": {
						"value": "[parameters('AzureSynapseAnalytics1_stackoverflow_connectionString')]"
					},
					"AzureDataLakeStorage1_GitHub_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_GitHub_properties_typeProperties_url')]"
					},
					"RestService1_Company_detail_properties_typeProperties_url": {
						"value": "[parameters('RestService1_Company_detail_properties_typeProperties_url')]"
					},
					"local_ADLS_project1_properties_typeProperties_url": {
						"value": "[parameters('local_ADLS_project1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}